import random
code=""
AT=40
for i in range (1,5001):
   	    x=random.randint(0,100)
   	    if x<AT/2:
           	code= code + 'A'
   	    if x>AT/2 and x<AT:
           	code = code + 'T'
            if x>AT and x<AT + (100-AT)/2: 
                code = code + 'G'
            if x>70:
                code = code + 'C'


 
print code
